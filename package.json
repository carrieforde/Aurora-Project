{
  "name": "aurora",
  "version": "0.1.0",
  "description": "A clean, modern, design-system focused project boilerplate that's both Sass and ES6 ready.",
  "main": "app.js",
  "directories": {
    "doc": "docs"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.9",
    "css-mqpacker": "^6.0.1",
    "cssnano": "^3.10.0",
    "eslint": "^4.14.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "img-loader": "^2.0.0",
    "jsdoc": "^3.5.5",
    "kss": "^3.0.0-beta.18",
    "node-sass": "^4.7.2",
    "onchange": "^3.3.0",
    "postcss-loader": "^2.0.10",
    "prettier": "^1.9.2",
    "sanitize.scss": "^1.0.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "stylelint": "^8.4.0",
    "stylelint-config-recommended-scss": "^3.0.0",
    "stylelint-order": "^0.8.0",
    "stylelint-scss": "^2.2.0",
    "stylelint-webpack-plugin": "^0.10.1",
    "svg-sprite-loader": "^3.6.2",
    "svgo": "^1.0.3",
    "svgo-loader": "^2.1.0",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "dev": "webpack-dev-server --open",
    "eslint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "eslint:fix": "eslint \"src/**/*.{js,jsx}\" --quiet --fix",
    "format": "prettier --write --single-quote --print-width=120 \"src/**/*.{js,jsx}\"",
    "jsdoc": "jsdoc src/assets/scripts/*.js -d docs/jsdoc",
    "kss": "kss --config kss-config.json",
    "ksswatch": "onchange 'src/assets/sass/**/*.scss' -- npm run kss",
    "sassdoc": "sassdoc src/assets/sass/**/*.scss -d docs/sassdoc",
    "stylelint": "stylelint \"src/**/*.{css,scss}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss}\" --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carrieforde/Aurora.git"
  },
  "keywords": [
    "sass",
    "javascript",
    "es6",
    "eslint",
    "stylelint",
    "kss",
    "design-system"
  ],
  "author": "Carrie Forde",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/carrieforde/Aurora/issues"
  },
  "homepage": "https://github.com/carrieforde/Aurora#readme"
}
